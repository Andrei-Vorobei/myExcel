{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest/globals": true
	},
	"rules": {
		"indent": "off",
		"linebreak-style": "off",
		"no-tabs": "off",
		"require-jsdoc": "off",
		"eol-last": "off",
		"comma-dangle": "off",
		"arrow-parens": "off",
		"max-len": "off",
		"object-curly-spacing": "off",
		"no-fallthrough": "off"
	},
	"plugins": ["jest"],
	"extends": ["eslint:recommended", "google"]
}